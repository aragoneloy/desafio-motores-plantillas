
<div class="container mt-3">
            <h2 style="color:crimson;">Historial</h2>
            {{#if objs}}
            <div class="table-responsive">
                <table class="table table-dark ">
                    <tr style="color: yellow;"> <th>Producto</th> <th>Precio</th> <th>Foto</th> </tr>
                    {{#each objs}}
                    <tr>
                        <td>{{this.title}}</td>
                        <td>{{this.price}}</td>
                        <td><img width="100" src={{this.thumbnail}} alt=""></td>
                    </tr>
                    {{/each}}
                </table>
            </div>
            {{/if}}
</div>
